---
import { Image } from 'astro:assets';

interface Props {
  title: string;
}
const { title } = Astro.props;
---

<article class="c--card-c">
  <Image
    class="c--card-c__brand"
    src="/assets/terra.gif"
    alt="Animated Terra HQ logo"
    loading="lazy"
    width="165"
    height="48.18"
  />
  <h3 class="c--card-c__title">{title}</h3>
</article>
